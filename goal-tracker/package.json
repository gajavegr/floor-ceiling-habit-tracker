{
  "name": "goal-tracker",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "version": "1.0.0",
  "description": "v1.0.0 setting up empty app",
  "scripts": {
    "start": "npm run start --workspace=@goal-tracker/client",
    "start:debug": "PORT=3000 DEBUG=true react-scripts start",
